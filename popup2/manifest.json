{
    "manifest_version": 2,
    "name": "popup ext2",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "permissions": [
        "tabs"
    ],
    "browser_action": {
        "default_icon": "plant_icon.png",
        "default_popup": "bounce/index.html",
        "default_title": "bounce"
    }
}